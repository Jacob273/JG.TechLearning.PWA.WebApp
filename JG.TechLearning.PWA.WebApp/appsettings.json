{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "pwa": {
    "cacheId": "Worker 1.15",
    "registerWebmanifest": true,
    "registerServiceWorker": false,
    "routesToPreCache": "/",
    "strategy": "cacheFirst",
    "offlineRoute": "/fallback.html"
  }
}
